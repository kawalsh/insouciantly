<% content_for :title do %>
	<h3 class="logo"><span class="verb">[about]</span>Insouciantly </h3>
<% end %>
<% content_for :fizz do  %>
	<span class = "fizztext">
		K.W.
	</span>
	<%= image_tag("about.jpg", :class => "fizz") %>
<% end %>
<h1> About Me </h1>

<% content_for :sidebar do %>
	<h2><%= link_to "More About Me", "www.google.com" %></h2>
	<% @about_feed.each do |blog| %>
		<div class="date"><%= blog.posted_at.strftime("%m/%d/%y")%></div>
		<%= image_tag(blog.fizz_photo.filepath, :width => "100%") %>
		<b><%= link_to blog.title, blog_url(blog) %></b><br />
		<%= blog.body[0,130].html_safe %>
		<hr />
	<% end %>
<% end %>
